<Window x:Name="DeleteEntry" x:Class="CaptainsLog.DeleteEntryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CaptainsLog"
        mc:Ignorable="d"
        Title="Delete Entry" Height="150" Width="400" MinWidth="400" MinHeight="150" MaxWidth="400" MaxHeight="150">
    <Grid>
        <TextBlock x:Name="textBlock" Margin="10,10,0,10" TextWrapping="Wrap" Text="Enter log entry ID number you would like to delete:" HorizontalAlignment="Left" Width="111"/>
        <TextBox x:Name="textBox" Margin="0,10,10,32" TextWrapping="Wrap" HorizontalAlignment="Right" Width="256"/>
        <Button x:Name="button" Content="Delete" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Cancel" Margin="0,39,90,10" Click="button1_Click" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
